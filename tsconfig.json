{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components": ["components/index.ts"],
      "@constants": ["constants/index.ts"],
      "@hooks": ["hooks/index.ts"],
      "@navigators/*": ["navigators/*"],
      "@screens": ["screens/index.ts"],
      "@stores/*": ["stores/*"],
      "@themes": ["themes/index.ts"],
      "@types": ["types/index.ts"],
      "@utils/*": ["utils/*"]
    }
  }
}
